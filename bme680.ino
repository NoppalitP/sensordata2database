void drawTemp(void) {
  int i = 0;
  while(i < 20){
        if (! bme.performReading()) {
    Serial.println("Failed to perform reading :(");
    return;
  }
  u8g2.clearBuffer();
  u8g2.setFont(u8g2_font_unifont_tf);
  u8g2.setCursor(0, 10);
  u8g2.print("TEMPERATURE  1/5");

  u8g2.setFont(u8g2_font_fub20_tf);

  u8g2.setCursor(5, 48);


  u8g2.print(String(round(bme.temperature * 10) / 10, 1) + " C");


  u8g2.drawCircle(65, 23, 2);

 
  u8g2.drawXBMP(100, 16, 25, 47, all_bm_temp[i]);
  i = i +1 ;
  u8g2.sendBuffer();
  delay(300);
}}

